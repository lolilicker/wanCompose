<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.WanJetpackCompose" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimaryVariant">@color/blue10_EFF2F3</item>
        <item name="colorOnPrimary">@color/white_ffffff</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/gray_a6a6a6</item>
        <item name="colorSecondaryVariant">@color/gray50_dddddd</item>
        <item name="colorOnSecondary">@color/gray_black50_777777</item>
        <!-- Status bar color. -->
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/white_ffffff</item>
        <item name="colorPrimaryDark">@color/blue5_f8f9f9</item>
        <item name="colorAccent">@color/blue_n</item>
        <item name="android:statusBarColor">@color/blue5_f8f9f9</item>
    </style>

    <style name="Theme.WanJetpackCompose.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.WanJetpackCompose.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.WanJetpackCompose.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="ThemeOverlay.WanJetpackCompose.FullscreenContainer" parent="">
        <item name="fullscreenBackgroundColor">@color/white_ffffff</item>
        <item name="fullscreenTextColor">@color/gray_black_333333</item>
    </style>
</resources>